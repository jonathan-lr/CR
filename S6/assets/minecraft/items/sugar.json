{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "fallback": {
      "type": "model",
      "model": "item/sugar"
    },
    "cases": [
      {
        "when": "coke",
        "model": {
          "type": "model",
          "model": "item/drugs/coca"
        }
      },
      {
        "when": "cokeground",
        "model": {
          "type": "model",
          "model": "item/drugs/coca_ground"
        }
      },
      {
        "when": "cokemixed",
        "model": {
          "type": "model",
          "model": "item/drugs/coca_mixed"
        }
      }
    ]
  }   
}