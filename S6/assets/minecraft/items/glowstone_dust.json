{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "fallback": {
      "type": "model",
      "model": "item/glowstone_dust"
    },
    "cases": [
      {
        "when": "opium",
        "model": {
          "type": "model",
          "model": "item/drugs/opium"
        }
      },
      {
        "when": "opiumsap",
        "model": {
          "type": "model",
          "model": "item/drugs/opium_sap"
        }
      },
      {
        "when": "opiumresin",
        "model": {
          "type": "model",
          "model": "item/drugs/opium_resin"
        }
      },
      {
        "when": "opiumcompact",
        "model": {
          "type": "model",
          "model": "item/drugs/opium_compact"
        }
      }
    ]
  }
}
