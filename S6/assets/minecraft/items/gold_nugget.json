{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "fallback": {
      "type": "model",
      "model": "item/gold_nugget"
    },
    "cases": [
      {
        "when": "sky",
        "model": {
          "type": "model",
          "model": "item/custom/dungeontoken/dungeonrewardair1"
        }
      },
      {
        "when": "earth",
        "model": {
          "type": "model",
          "model": "item/custom/dungeontoken/dungeonrewardearth2"
        }
      },
      {
        "when": "water",
        "model": {
          "type": "model",
          "model": "item/custom/dungeontoken/dungeonrewardwater4"
        }
      },
      {
        "when": "fire",
        "model": {
          "type": "model",
          "model": "item/custom/dungeontoken/dungeonrewardfire3"
        }
      }
    ]
  }   
}